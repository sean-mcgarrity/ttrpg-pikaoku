{
  "extends": "./.svelte-kit/tsconfig.json",
  "compilerOptions": {
    "baseUrl": ".",
    "allowSyntheticDefaultImports": true,
    "paths": {
      "$assets/*": ["./src/assets/*"],
      "$src/*": ["./src/*"],
      "$lib/*": ["src/lib/*"],
      "$components/*": ["src/components/*"],
      "$utils/*": ["src/utils/*"]
    }
  }
}
